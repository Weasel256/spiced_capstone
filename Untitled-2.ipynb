   "source": [
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Your specified color\n",
    "base_color = \"#d4bafc\"\n",
    "\n",
    "# Create a colormap using seaborn\n",
    "cmap = sns.color_palette([base_color, sns.desaturate(base_color, 0.5)])\n",
    "\n",
    "# Display the colormap\n",
    "sns.palplot(cmap)\n",
    "plt.show()\n"
   ]
  }
